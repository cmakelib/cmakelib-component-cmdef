//
// Test Resource File for CMDEF_ADD_EXECUTABLE Tests
//
// This is a minimal Windows resource file used for testing
// resource file handling in CMDEF_ADD_EXECUTABLE test cases.
//

#include <windows.h>

// Version Information
VS_VERSION_INFO VERSIONINFO
FILEVERSION     1,0,0,0
PRODUCTVERSION  1,0,0,0
FILEFLAGSMASK   VS_FFI_FILEFLAGSMASK
FILEFLAGS       0x0L
FILEOS          VOS__WINDOWS32
FILETYPE        VFT_APP
FILESUBTYPE     VFT2_UNKNOWN
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "CompanyName",      "CMDEF Test Company"
            VALUE "FileDescription", "CMDEF Test Executable"
            VALUE "FileVersion",     "1.0.0.0"
            VALUE "InternalName",    "cmdef_test"
            VALUE "LegalCopyright",  "Copyright (C) 2024 CMDEF Test"
            VALUE "OriginalFilename", "cmdef_test.exe"
            VALUE "ProductName",     "CMDEF Test Product"
            VALUE "ProductVersion",  "1.0.0.0"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END

// Application Icon
IDI_APPLICATION ICON "test_icon.ico"

// String Table
STRINGTABLE
BEGIN
    IDS_APP_TITLE       "CMDEF Test Application"
    IDS_APP_DESCRIPTION "Test application for CMDEF_ADD_EXECUTABLE"
END

// Menu Resource
IDR_MAINMENU MENU
BEGIN
    POPUP "&File"
    BEGIN
        MENUITEM "&Exit",                       ID_FILE_EXIT
    END
    POPUP "&Help"
    BEGIN
        MENUITEM "&About",                      ID_HELP_ABOUT
    END
END

// Dialog Resource
IDD_ABOUTBOX DIALOG 0, 0, 200, 100
STYLE DS_MODALFRAME | WS_POPUP | WS_CAPTION | WS_SYSMENU
CAPTION "About CMDEF Test"
FONT 8, "MS Sans Serif"
BEGIN
    DEFPUSHBUTTON   "OK",IDOK,75,80,50,14
    CTEXT           "CMDEF Test Application",IDC_STATIC,50,20,100,8
    CTEXT           "Version 1.0",IDC_STATIC,50,35,100,8
    CTEXT           "Test resource file for CMDEF",IDC_STATIC,25,50,150,8
END

// Resource IDs
#define IDS_APP_TITLE           101
#define IDS_APP_DESCRIPTION     102
#define IDR_MAINMENU           103
#define IDD_ABOUTBOX           104
#define ID_FILE_EXIT           105
#define ID_HELP_ABOUT          106
